#wpadminbar
  direction ltr
  height: adminbar.height
  position fixed
  top 0
  left 0
  width 100%
  z-index 139
  user-select none
  box-shadow 0px 2px 1px -2px rgba(black, .5)
  padding-left 15px

  +above(l)
    padding-left 0

  *
    text-align left
    font-size 14px
    line-height: adminbar.height
    vertical-align top
    margin 0
    outline none
    float none

  ul
    padding-left 0
    list-style-type none

  li
    display inline-block

  a
    text-decoration none

  #wp-admin-bar
    &-root
      &-default
        > li
          height: adminbar.height
          vertical-align top

          > .s_live_icon.ab-item:before
            content attr(data-content) !important

    &-view
      > a:before
        content '\ed33'

    &-bp-notifications
      .no-new + .ab-sub-wrapper
        display none!important

    &-updates a.ab-item
      padding 0!important
      position relative

    &-user-info
      .display-name
      .username
        display block
        text-align:center
        vertical-align top
        line-height 20px

    &-top-secondary:before
      content ''
      width 10px
      height 100%

    &-search
      position absolute
      right 15px
      top 55px
      z-index 20

      input
        background none
        padding 0
        border none!important
        color rgba(black, .65)

        &#adminbar-search
          margin 0
          line-height 30px
          vertical-align top
          text-transform none
          letter-spacing 0
          font-size 13px
          font-weight 600
          max-width 130px
          width 0
          transition width .15s ease-in-out

        &[type="submit"]
          position absolute
          right 0
          margin-right 0
          text-indent 99999px
          width 28px
          z-index 11

      form
        color rgba(black, .65)
        border 1px solid
        display block
        border-radius 40px
        max-height 32px
        line-height 32px
        height 32px
        padding 0 10px
        min-width 32px
        position relative

        &:after
          content '\ec2c'
          font 600 12px/30px streamline
          position absolute
          right 0
          z-index 10
          width 28px
          text-align center

        &:hover
          input#adminbar-search
            width 130px
            margin-right 30px

    &-new-content
      >.ab-item .ab-icon
        display none

      ul:not(.sticky_icon_picker) li:not(.sticky-menu-head)
        +above(l)
          display block
          float left
          width 25%
          text-align center
          position relative

      &-default
        li#wp-admin-bar-new
          &-post a:before
            content '\e8a4'

          &-media a:before
            content '\ebc1'

          &-page a:before
            content '\e8a5'

          &-user a:before
            content '\eb91'

          &-forum a:before
            content '\e83f'

          &-topic a:before
            content '\e976'

          &-reply a:before
            content '\e73e'

    &-my-account
      margin-right 0!important

      &:before
        content ''
        height: adminbar.height
        width 1px
        position absolute
        left -2px
        top 0px

      em
        line-height: adminbar.height
        font-size 8px
        display inline-block
        vertical-align top
        opacity .65
        margin-right 5px
        text-transform uppercase
        display none

      > a
        display inline-block
        line-height: adminbar.height
        height: adminbar.height
        vertical-align top
        text-align left
        position relative
        text-transform capitalize
        font-size 0
        font-weight 400
        letter-spacing .4px
        opacity 1
        margin 0 15px
        transform translateY(0)
        transition transform .25s ease-in-out, opacity .25s ease-in-out

        &:after
          font 400 6px/6px streamline
          content '\e666'
          margin-left 10px
          display inline-block
          vertical-align middle!important

      &:hover
        > a
          transform translateY(-32px)
          opacity 0

      &.with-avatar
        > a img
          size 22px
          border-radius 22px
          line-height 22px
          vertical-align middle
          margin 0 15px 0 0
          display inline-block

      &-buddypress
        display block

        li
          display block

      .ab-sub-wrapper
        a
          display block
          position relative

          .count
            float right
            line-height 18px

        &:before
          display none
          content ''
          border-radius 0

  .ab-top-secondary
    height 100% !important
    position absolute
    right 0

    > li
      border 0
      display block
      height 100%
      float right
      margin-right 15px
      padding 0

      &:first-child
        margin-right 0

      > a.new
        opacity 1

      > a
      > .ab-empty-item
        border 0
        opacity .25

    .menupop
      > .ab-sub-wrapper
        right 0
        left 0
        top 44px
        min-width 0
        border-radius 0
        box-shadow -2px 2px 0 rgba(black, .1)
        opacity 1
        transform translate(40px, 0) scale(0)
        transition transform .25s ease-in-out

      &:hover
        > .ab-sub-wrapper
          display block
          opacity 1
          transform translate(0px, 0px) scale(1)

      &:not(#wp-admin-bar-new-content)
        ul:not(#wp-admin-bar-user-actions):not(#sticky_view_site)
          li
            .ab-item:before
              content '\e695'
              font 400 4px/14px streamline
              display inline-block
              vertical-align top
              position absolute
              left 10px
              top 10px
              transition font-size .1s ease-in-out

  .quicklinks
    .screen-reader-text
      display none

    > ul
      text-align left
      display inline-block
      height 100%
      line-height: adminbar.height
      vertical-align top
      margin 0
      white-space nowrap

      &:first-child
        max-width 70%
        float left

  #sticky
    &_toggle
      position fixed
      top 43px
      left calc( 50% - 30px )
      z-index 85
      height 16px
      padding 0 5px
      border-bottom-radius 2px
      transition transform .15s ease-in-out, opacity .15s linear

      button
        border 0!important
        padding 0!important
        vertical-align top
        width 16px
        margin -1px 0px
        height 16px
        text-align center
        display inline-block
        cursor pointer

    &_resize
      &:before
        content ''
        display block
        position absolute
        top 0
        left 0
        width 52.5%
        height 100%
        box-shadow 0px 2px 1px -2px rgba(black, .5)
        border-radius 0 0 0 5px
        transform skewX(10deg)
        z-index -1

    &_close
      &:after
        content ''
        display block
        position absolute
        top 0
        right 0
        width 52.5%
        height 100%
        box-shadow 0px 2px 1px -2px rgba(black, .5)
        border-radius 0 0 5px 0
        transform skewX(-10deg)
        z-index -1

    &_resize:before
    &_close:after
      background-color:inherit


